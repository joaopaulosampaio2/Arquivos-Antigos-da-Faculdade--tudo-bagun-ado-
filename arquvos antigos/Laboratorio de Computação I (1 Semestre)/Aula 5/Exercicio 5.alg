algoritmo "Ex 5 Expoente (Aula5)"

var
conta, a, b, n, count, aux : inteiro
inicio
      escreval("Digite um valor: ")
        leia (a)
      escreval("Digite o valor que será o expoente do valor anterior: ")
        leia (b)
       conta <- 1
      para count de 1 ate b faca
      conta <- conta * a

      fimpara
      escreval(conta)

//João Paulo Theodoro
//João Vitor leite

fimalgoritmo
