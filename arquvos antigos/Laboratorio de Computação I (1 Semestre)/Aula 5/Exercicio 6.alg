algoritmo "Exercício 6 (Aula5)"
var
x, y, c : inteiro
inicio
      escreval ("informe valor de X ")
      leia(x)
      y <- 1
      para c de 1 ate 10 passo 1 faca
      c <- x + c
      y <- y * c
      fimpara
      escreval ("resultado: y =", y)
      
//João Paulo Theodoro
//João Vitor leite
      
fimalgoritmo
